{
  "pages": [
    {
      "url": "https://www.bbc.com/news/world",
      "path_patterns": [
        "/news/world-europe-67529571"
      ],
      "article_selectors": {
        "container": {
          "selector": "#main-content"
        },
        "components": [
          {
            "key": "title",
            "selector": "#main-heading"
          },
          {
            "key": "author",
            "selector": "[data-component=byline-block] [class*=TextContributorName]"
          },
          {
            "key": "publish_date",
            "selector": "[data-testid=timestamp]",
            "extract": {
              "type": "attribute",
              "key": "datetime"
            }
          },
          {
            "type": "multiple",
            "key": "article",
            "selector_list": [
              {
                "type": "multiple",
                "key": "image_with_src",
                "selector_list": [
                  {
                    "key": "image_src",
                    "selector": "img[class*=-Image]",
                    "extract": {
                      "type": "attribute",
                      "key": "src"
                    }
                  },
                  {
                    "key": "image_alt",
                    "selector": "img[class*=-Image]",
                    "extract": {
                      "type": "attribute",
                      "key": "alt"
                    }
                  }
                ]
              },
              {
                "key": "image",
                "selector": "[data-component=image-block]",
                "extract": {
                  "type": "html"
                }
              },
              {
                "key": "paragraph",
                "selector": "[data-component=text-block]"
              }
            ]
          }
        ] 
      }
    }
  ]
}